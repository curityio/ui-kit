{
  "name": "@curity-ui-kit/css",
  "version": "1.0.0",
  "description": "Curity CSS library",
  "main": "index.js",
  "workspaces": [
    "lib/*",
    "docs/*"
  ],
  "scripts": {
    "start:docs": "npm start --prefix docs",
    "start:lib": "npm start --prefix lib",
    "start": "npm-run-all -p start-docs start-lib",
    "build:docs": "npm run build --prefix docs",
    "build:lib": "npm run build --prefix lib",
    "build": "npm-run-all -s build:lib build:docs"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org-urbansanden:curity/curity-css.git"
  },
  "author": "Curity",
  "license": "ISC",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
