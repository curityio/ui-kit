{
  "name": "@curity-ui-kit/identity-server",
  "author": "Curity",
  "version": "1.6.0",
  "private": "true",
  "engines": {
    "node": ">=20.16.0",
    "npm": ">=10.8.1"
  },
  "scripts": {
    "java": "node previewer",
    "dev": "webpack --config webpack.config.js --mode development --stats-error-details",
    "prestart": "npm run build",
    "start": "concurrently --kill-others \" npm run java\" \" npm run dev\"",
    "build": "webpack --config webpack.config.js --mode production",
    "clean": "rm -rf build",
    "lint:styles": "stylelint ./src/curity/scss/**/*.scss --custom-syntax postcss-scss",
    "lint:styles:fix": "stylelint ./src/curity/scss/**/*.scss --custom-syntax postcss-scss --fix",
    "precopy": "npm run build",
    "copy": "webpack --config copy.config.js",
    "preinstall": "cp ./git/hooks/* .git/hooks/ || true"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "del": "^5.1.0",
    "dotenv": "^16.3.1",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "front-matter": "^4.0.2",
    "html-webpack-plugin": "^5.6.0",
    "jquery": "3.5.1",
    "mini-css-extract-plugin": "^2.7.6",
    "node-env-file": "^0.1.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-loader": "^7.3.4",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.7.2"
  }
}
