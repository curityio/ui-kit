{
  "name": "@curity-ui-kit/identity-server",
  "author": "Curity",
  "version": "1.6.0",
  "engines": {
    "node": ">=20.16.0",
    "npm": ">=10.8.1"
  },
  "scripts": {
    "java": "node previewer",
    "prestart": "npm run build",
    "clean": "rm -rf build",
    "preinstall": "cp ./git/hooks/* .git/hooks/ || true",
    "start": "concurrently --kill-others \" npm run java\" \" npm run dev\"",
    "dev:lib": "vite --config vite.lib.config.ts",
    "dev:styles": "vite --config vite.styles.config.ts",
    "dev": "concurrently \"npm:dev:lib\" \"npm:dev:styles\"",
    "prebuild": "npm run build --workspace @curity-ui-kit/css || true",
    "build": "vite build -c vite.lib.config.ts && vite build -c vite.styles.config.ts"
  },
  "dependencies": {
    "jquery": "3.5.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^7.1.5",
    "vite-plugin-static-copy": "^3.1.2",
    "browser-sync": "^3.0.2",
    "node-env-file": "^0.1.8"
  }
}
